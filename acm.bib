}

@article{10.1145/3620668,
author = {Gemeinhardt, Felix and Garmendia, Antonio and Wimmer, Manuel and Weder, Benjamin and Leymann, Frank},
title = {Quantum Combinatorial Optimization in the NISQ Era: A Systematic Mapping Study},
year = {2023},
issue_date = {March 2024},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {56},
number = {3},
